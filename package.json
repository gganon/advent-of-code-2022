    "day9": "yarn run-ts src/day9/rope",
    "day10": "yarn run-ts src/day10/clock",
    "day11": "yarn run-ts src/day11/monkeys",
    "day12": "yarn run-ts src/day12/hill",
    "day13": "yarn run-ts src/day13/distress",
    "day14": "yarn run-ts src/day14/sand"
  },
  "dependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.10",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
